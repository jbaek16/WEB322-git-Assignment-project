<body>
    <header>
        <div id="section" class="hero-package">
            <div class="package" style="width: 1000px; height: auto; overflow: hidden; display: block; margin: 0 auto;">
                <img src="../img/package.png" alt="main Images"
                    style="width: 900px; height: auto;display: block; margin: 0 auto;">
            </div>
        </div>
    </header>

    <p style="text-align:center; font-size:40px;">Pick a package, choose your meals, achieve your goal - </p>
    <b>
        <p style="font-style: italic;text-align:center; font-size:40px;">it's that easy!</p>
    </b>

    <main>
        <div class="container">
            {{#each data}}

            <div class="" style="padding:130px; float:left;width:140px; height: 140px;display: block; margin: 0 auto;">
                <img src="../img/{{this.imgPath}}"
                    style=" float:left; width: 140px; height: 140px ;display: block; margin: 0 auto;"> <br>
                <b>
                    <p style="text-align:center; font-size:20px;">{{this.title}}
                </b></p><br>
                {{this.description}}
                <p style="font-style: italic;font-size:15px;text-align:center;">from</p> <b>
                    <p style="text-align:center;"> {{this.price}}</p>
                </b><br><br>

            </div>

            {{/each}}
        </div>
    </main>
</body>